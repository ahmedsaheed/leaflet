{
  "build": {
    "appId": "com.leaflet",
    "productName": "Leaflet",
    "copyright": "Copyright © 2022 Ahmed Saheed",
    "mac": {
      "icon": "./resources/app.icns",
      "publish": "github"
    },
    "win": {
      "icon": "./resources/app.ico",
      "publish": "github"
    },
    "linux": {
      "icon": "./resources/icon.png",
      "publish": "github",
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "snap"
      ]
    },
    "directories": {
      "output": "dist",
      "buildResources": "resources"
    }
  },
  "name": "leaflet",
  "productName": "Leaflet",
  "description": "Leaflet is a simple minimal markdown editor",
  "version": "0.0.5",
  "author": "Ahmed Saheed <ahmedsaheed2@outlook.com>",
  "copyright": "Copyright © 2022 Ahmed Saheed",
  "main": "app/background.js",
  "license": "CC-BY-NC-SA-4.0",
  "scripts": {
    "dev": "nextron",
    "electron-trace": "electron --trace-warnings .",
    "build": "nextron build",
    "build:all": "nextron build --all",
    "build:win32": "nextron build --win --ia32",
    "build:win64": "nextron build --win --x64",
    "build:mac": "nextron build --mac --x64",
    "build:linux": "nextron build --linux",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder -m -l -w --publish always",
    "release:win32": "electron-builder --publish always --win --ia32",
    "release:win64": "electron-builder --publish always --win --x64",
    "release:mac": "electron-builder --publish never --mac --x64",
    "release:linux": "electron-builder --publish always --linux"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmedsaheed/Leaflet.git"
  },
  "dependencies": {
    "@babel/parser": "^7.21.3",
    "@codemirror/commands": "^6.1.3",
    "@codemirror/highlight": "^0.19.8",
    "@codemirror/lang-markdown": "^6.0.3",
    "@codemirror/language": "^6.3.0",
    "@codemirror/language-data": "^6.1.0",
    "@codemirror/search": "^6.3.0",
    "@codemirror/state": "^6.1.2",
    "@codemirror/view": "^6.4.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@excalidraw/excalidraw": "^0.14.2",
    "@lezer/common": "^1.0.2",
    "@lezer/highlight": "^1.1.3",
    "@replit/codemirror-indentation-markers": "^6.3.0",
    "@replit/codemirror-vim": "^6.0.3",
    "@types/markdown-it": "^12.2.3",
    "@uiw/codemirror-theme-github": "^4.12.4",
    "@uiw/react-codemirror": "^4.12.4",
    "chokidar": "^3.5.3",
    "cm6-theme-basic-light": "^0.2.0",
    "date-fns": "^2.29.3",
    "directory-tree": "^3.3.1",
    "drag-drop": "^7.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.0.1",
    "framer-motion": "^10.0.0",
    "fs-extra": "^10.1.0",
    "highlight.js": "^11.6.0",
    "highlightjs-line-numbers.js": "^2.8.0",
    "katex": "^0.16.3",
    "lodash.clonedeep": "^4.5.0",
    "log4js": "^6.9.1",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-attrs": "^4.1.4",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-container": "^3.0.0",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-gridtables": "^0.5.0",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-metadata-block": "^1.0.2",
    "markdown-it-pandoc": "^2.1.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-texmath": "^1.0.0",
    "mermaid": "^10.3.1",
    "mime": "^3.0.0",
    "moment": "^2.29.4",
    "open": "^8.4.0",
    "prettier": "^2.8.4",
    "react-cmdk": "^1.3.6",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.7.1",
    "reading-time": "^1.5.0",
    "remark-parse": "^10.0.1",
    "styled-components": "^5.3.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "autoprefixer": "^10.4.7",
    "electron": "^18.2.0",
    "electron-builder": "^23.3.3",
    "next": "^12.1.6",
    "nextron": "^8.1.0",
    "postcss": "^8.4.13",
    "react": "^18.2.0",
    "react-dom": "^18.1.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.8.4"
  }
}
